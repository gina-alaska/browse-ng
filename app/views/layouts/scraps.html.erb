<!DOCTYPE html>
<html>
<head>
  <title>Browse.Alaskamapped.Org</title>
  <%= stylesheet_link_tag    "map", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "map", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/analytics' %>
  <section id="header">
    <%= render 'layouts/header' %>
  </section>
  <section id="map-container" class="full-screen">
    <%= yield :map_container %>
  </section>
  <%= yield %>
  <%= render 'layouts/login' %>      
</body>
</html>
